main.Name = "MainFrame"
main.Parent = gui
main.Size = UDim2.new(0, 720, 0, 420)
main.Position = UDim2.new(0.5, -360, 0.5, -210)
main.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
main.Active = true
main.Draggable = true 
