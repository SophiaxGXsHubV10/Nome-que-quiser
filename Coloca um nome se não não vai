toggle.Position = UDim2.new(0.02, 0, 0.45, 0)
toggle.BackgroundColor3 = Color3.fromRGB(35, 30, 50)
toggle.BorderSizePixel = 0
toggle.ZIndex = 1000
toggle.Draggable = true -- Nota: Ativar se seu executor suportar, sen√£o requer script de arrasto

--// ID DA IMAGEM (Troque 6031094678 pelo ID da imagem SFz Hub que voc√™ upar)
toggle.Image = "rbxassetid://6031094678" 
toggle.ScaleType = Enum.ScaleType.Fit

local toggleCorner = Instance.new("UICorner", toggle)
toggleCorner.CornerRadius = UDim.new(1, 0)

local toggleStroke = Instance.new("UIStroke", toggle)
toggleStroke.Color = Color3.fromRGB(140, 80, 255)
toggleStroke.Thickness = 3

--// A√ß√£o de Abrir/Fechar
toggle.MouseButton1Click:Connect(function()
    main.Visible = not main.Visible
    if main.Visible then
        toggleStroke.Color = Color3.new(1, 1, 1)
    else
        toggleStroke.Color = Color3.fromRGB(140, 80, 255)
    end
end)

