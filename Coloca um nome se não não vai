--// Fun√ß√£o para criar bot√µes de script dentro das p√°ginas
local function createScriptPage(page)
    local sf = Instance.new("ScrollingFrame")
    sf.Parent = page
    sf.Size = UDim2.new(1, -20, 1, -60)
    sf.Position = UDim2.new(0, 10, 0, 50)
    sf.BackgroundTransparency = 1
    sf.BorderSizePixel = 0
    sf.ScrollBarThickness = 4
    sf.CanvasSize = UDim2.new(0, 0, 0, 0)

    local sl = Instance.new("UIListLayout", sf)
    sl.Padding = UDim.new(0, 8)
    sl.HorizontalAlignment = Enum.HorizontalAlignment.Center

    sl:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        sf.CanvasSize = UDim2.new(0, 0, 0, sl.AbsoluteContentSize.Y + 10)
    end)

    return function(text, callback)
        local btn = Instance.new("TextButton", sf)
        btn.Size = UDim2.new(1, -10, 0, 38)
        btn.Text = text
        btn.BackgroundColor3 = Color3.fromRGB(60, 40, 110)
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 14
        Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)
        btn.MouseButton1Click:Connect(function() pcall(callback) end)
    end
end

